{
  "$schema": "./node_modules/@nrwl/schematics/src/schema.json",
  "project": {
    "name": "angular-training",
    "npmScope": "angular-training",
    "latestMigration": "20180227-cleanup-scripts"
  },
  "e2e": {},
  "lint": [
    {
      "project": "./tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {},
  "apps": [
    {
      "name": "api",
      "root": "libs/api/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "crud",
      "root": "libs/crud/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "entity-repository",
      "root": "libs/entity-repository/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "ngrx-helpers",
      "root": "libs/ngrx-helpers/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "router",
      "root": "libs/router/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "$workspaceRoot",
      "root": ".",
      "appRoot": ""
    }
  ],
  "defaults": {
    "schematics": {
      "collection": "@nrwl/schematics"
    },
    "styleExt": "scss",
    "component": {},
    "build": {
      "preserveSymlinks": true
    }
  },
  "warnings": {
    "typescriptMismatch": false
  }
}
